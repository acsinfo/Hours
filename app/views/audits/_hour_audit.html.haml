.entry
  %div.title-container
    %p.entry-title Hours Entry
  %div
    %span= t("audits.starting_time")
    %span= I18n.l(@history.auditable.starting_time)
  %div
    %span= t("audits.user")
    %span= @history.auditable.user.name
  %div
    %span= t("audits.project")
    %span= @history.auditable.project.name
  %div
    %span= t("audits.category")
    %span= @history.auditable.category.name if @history.auditable.category
  %div
    %span= t("audits.hours")
    %span= @history.auditable.value
= render partial: "audits"
